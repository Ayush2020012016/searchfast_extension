{
   "manifest_version":2,
   "name":"searchfast",
   "version":"0.1",
   "description":"quick searchbar",
   "icons":{"128":"logo.png"},
   "content_scripts":[
       {
           "matches":["<all_urls>"],
           "js":["content.js"]
       }
   ],
   "background":{
    "scripts": [
       "background.js"
    ]
       
   },
   "browser_action":{
       "default_popup":"index.html",
       "default_title":"search",
       "default_icons":"logo.png"
   }

}